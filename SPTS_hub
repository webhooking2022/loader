    msg.Text = "You have "..Translate(game.Players.LocalPlayer.Character.Humanoid.Health) .. ' of health.\nI recommend your put: '.. Translate((game.Players.LocalPlayer.Character.Humanoid.Health / 5));
    game.Debris:AddItem(msg, 2)
