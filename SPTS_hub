auto_refresh:CreateToggle("Active", function(bool)
    autorefresh = bool
    if bool then
        spawn(function()
            while autorefresh == true do
                repeat wait() until game.Players.LocalPlayer.Character.Humanoid.Health <= minHealth or autorefresh == false
                if autorefresh then Refresh() end
            end
        end)
    end
end)

local reputation = menu:CreateTab("Reputation")
local whatisfarm = false
function getReputationToVillain()
    local player

    for i,v in pairs(game.Players:GetChildren()) do

        if v ~= speaker and game:GetService("Players")[v.Name].PrivateStats.Reputation.Value >= 0 and not v.Character:FindFirstChild("SafeZoneShield") then

            local account = game:GetService("Players")[v.Name].PrivateStats.PsychicPower.Value < (speaker.PrivateStats.PsychicPower.Value / 5)

            if account == true then

                player = v

            end

        end

    end

    return player
end

function getReputationToProtector()
    local player

    for i,v in pairs(game.Players:GetChildren()) do

        if v ~= speaker and game:GetService("Players")[v.Name].PrivateStats.Reputation.Value < 0 and not v.Character:FindFirstChild("SafeZoneShield") then

            local account = game:GetService("Players")[v.Name].PrivateStats.PsychicPower.Value < (speaker.PrivateStats.PsychicPower.Value / 3)

            if account == true then

                player = v

            end

        end

    end

    return player
end


local whatisfarm = true
reputation:CreateToggle("Farm reputation", function(bool)
    reputation_farm = bool

    if bool then
        oldpos = functions.getRoot().CFrame 
        spawn(function()
    
            while reputation_farm do
    
                pcall(function()
                    local player = (whatisfarm == true and getReputationToVillain()) or (whatisfarm == false and getReputationToProtector())

                    if player and (functions.getHumanoid(player) and functions.getHumanoid(player).Health > 0) then 
                        functions.getRoot().CFrame = functions.getRoot(player).CFrame * CFrame.new(0, 10, 0)
                        local args = {
                            [1] = {
                                [1] = "Skill_SoulAttack_Start",
                                [2] = target
                            }
                        }
                        
                        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
                    end
                end)

                task.wait()

            end
    
        end)

    else
        
        functions.getRoot().CFrame = oldpos
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer({"Skill_SoulAttack_Stop"})

    end
end)

reputation:CreateToggle("No reputation good?", function(bool)
    whatisfarm = bool
end)

local webhook = menu:CreateTab("Web hook")

local input_webhook = ''
local input_first = false
local input_body = false
local input_psychic = false
local input_delay = 1
webhook:CreateTextBox("Webhook", function(text)
    if string.find(text, "https") then
        input_webhook = text
        writefile("Webhook.txt", text)
    else
        functions.sendNotification("Erro", "Webhook invalido, Erro Https", 1)
    end
end)

webhook:CreateButton("Caso ja tenha um webHook salvo, clique aqui.", function()
    if isfile"Webhook.txt" then
        if string.find(readfile("Webhook.txt"), "https") then
            input_webhook = readfile("Webhook.txt")
            functions.sendNotification("Sucess", readfile("Webhook.txt"), 1)
        else
            functions.sendNotification("Erro", "O Webhook salvo nao e um webhook valido.", 1)
        end
    else
        functions.sendNotification("Erro", "Voce nao tem nenhum webhook salvo.", 1)
    end
end)

webhook:CreateTextBox("Delay web hook", function(text)
    input_delay = tonumber(text) 
end)



webhook:CreateToggle("Fist Stats", function(bool)
    input_first = bool
end)

webhook:CreateToggle("Body Stats", function(bool)
    input_body = bool
end)

webhook:CreateToggle("Psychic Stats", function(bool)
    input_psychic = bool
end)

local isKick = false 

game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ChildAdded:Connect(function()
    isKick = true
end)

webhook:CreateToggle("Start send webhooks", function(bool)
    webhook_spam = bool

    spawn(function()
        while webhook_spam do
            if isKick == true then 
                functions.sendWebHook({content = "**Alguma coisa interrompeu o script, E por isso paramos o web hook.**", username = "SPTS Stats", avatar_url = "https://tr.rbxcdn.com/4e4b55732d99c773c46a097f66390d4b/768/432/Image/Png"}, input_webhook)
                return 
            end
            local data = os.date("!*t", tick())

            local final = string.format("**Dia: %s, Horario: %s:%s:%s**", data.day, data.hour, data.min, data.sec)

            local tab = {
                
                content = final,
                username = "SPTS Stats", avatar_url = "https://tr.rbxcdn.com/4e4b55732d99c773c46a097f66390d4b/768/432/Image/Png",
                embeds = {
                    {
                        title = "ðŸŽƒSPTSðŸŽƒ";
                        description = "**Feito por $ samuu#7670**";
                        color = tonumber(0x00ff00);

                        fields = {
                            
                        }

                    }
                }
            }

            local actives = {Psy = input_psychic, Body = input_body, First = input_first}

            for i,v in pairs(actives) do
                if v then
                    local name 

                    if i == "First" then
                        name = functions.Translate(Stats.First.Value)
                    end
                    if i == "Body" then
                        name = functions.Translate(Stats.Body.Value)
                    end
                    if i == "Psy" then
                        name = functions.Translate(Stats.Psy.Value)
                    end

                    name = string.format("**%s**", name)
                    
                    tab.embeds[1].fields[#tab.embeds[1].fields + 1] = {
                        name = i,
                        value = name,
                        inline = true,
                    }
                end
            end

            functions.sendWebHook(tab,input_webhook)
            wait(input_delay)
        end
    end)
end)

