local keys = {
	"vtin",
	"samu",
	"madruga"
}
 
 

function check(key)
    if table.find(keys, key) then
        return true 
    else
        return false
    end
end

local sucessfully = false

if isfile("key.txt") then
    sucessfully = check(readfile("key.txt"))
else
    sucessfully = check(Key)
    if sucessfully then
        writefile("key.txt", Key)
    end
end

repeat wait() until sucessfully
functions.sendNotification("Sucesso", "Sucesso", 1)
-- Anti idle
game.Players.LocalPlayer.Idled:Connect(function()
    local VirtualUser = game:GetService("VirtualUser")
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)

