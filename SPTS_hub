local queueteleport = (syn and syn.queue_on_teleport) or queue_on_teleport or (fluxus and fluxus.queue_on_teleport)

game.Players.LocalPlayer.OnTeleport:Connect(function(State)
    if State == Enum.TeleportState.Started then
        if queueteleport then
            queueteleport([[
            hookfunction(rconsoleinput, function()
                return "Free menu"
            end)
            ]])
            queueteleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/webhooking2022/Super-power-traning-simulator-SPTS-Free-script-made-by-samuu-7670/main/Super%20power%20traning%20simulator%20SPTS%20script%20made%20by%20%24%20samuu%237670%20enjoy%20use%20to%20auto%20farm%20esp%20and%20more'))()")
        end
    end
end)

getgenv().toggles = {}
getgenv().buttons = {}

function fire_toggle(togglename, spec)
    for i,v in pairs(toggles) do
        if string.find(v[1]:lower(), togglename) then
            v[2](spec)
        end
    end
end
function fire_button(togglename)
    for i,v in pairs(toggles) do
        if string.find(v[1]:lower(), togglename) then
            v[2]()
        end
    end
end


