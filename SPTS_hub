local NC = false
local NC_Clip
local Player = game.Players.LocalPlayer
local Char = Player.Character or Player.CharacterAdded:wait()

local Mouse = Player:GetMouse()

local Speed = 2

local Service = game:GetService("ContextActionService")
local Camera = workspace.CurrentCamera
local Player = game:GetService("Players").LocalPlayer 
local Input = game:GetService("UserInputService") 
local Forward = false 
local Back = false 
local Left = false 
local Right = false 
local Up = false 
local Down = false 
local BodyPosition
local Menu
local Heart = {}
local VELOCIDADE = 5

Player.Chatted:Connect(function(cmd)
	cmd = cmd .. ' '
	cmd = cmd:split(" ")
	
	if cmd[1] == "/e" then
		if cmd[2] == "velo" then
			VELOCIDADE = tonumber(cmd[3])
		end
	end
end)
Input.InputBegan:Connect(function(Key,b)
    if b then return end
    if Key.KeyCode == Enum.KeyCode.LeftShift then
        VELOCIDADE+=25
    end
	if Key.KeyCode == Enum.KeyCode.W or Key.KeyCode == Enum.KeyCode.Up then 
		Forward = true 
	end 
	if Key.KeyCode == Enum.KeyCode.S or Key.KeyCode == Enum.KeyCode.Down then 
		Back = true 
	end 
	if Key.KeyCode == Enum.KeyCode.A or Key.KeyCode == Enum.KeyCode.Left then
		Left = true 
	end 
	if Key.KeyCode == Enum.KeyCode.D or Key.KeyCode == Enum.KeyCode.Right then 
		Right = true 
	end 
	if Key.KeyCode == Enum.KeyCode.Space then 
		Up = true 
	end             
	if Key.KeyCode == Enum.KeyCode.LeftControl then 
		Down = true 
	end 
end)
Input.InputEnded:Connect(function(Key, b)
    if b then return end
    if Key.KeyCode == Enum.KeyCode.LeftShift then
    VELOCIDADE-=25
    end
	if Key.KeyCode == Enum.KeyCode.W or Key.KeyCode == Enum.KeyCode.Up then 
		Forward = false 
	end 
	if Key.KeyCode == Enum.KeyCode.S or Key.KeyCode == Enum.KeyCode.Down then 
		Back = false 
	end 
	if Key.KeyCode == Enum.KeyCode.A or Key.KeyCode == Enum.KeyCode.Left then
		Left = false 
	end 
	if Key.KeyCode == Enum.KeyCode.D or Key.KeyCode == Enum.KeyCode.Right then 
		Right = false 
	end 
	if Key.KeyCode == Enum.KeyCode.Space then 
		Up = false 
	end 
	if Key.KeyCode == Enum.KeyCode.LeftControl then 
		Down = false 
	end 
end)
farms:CreateToggle("Fly", function(bool)
    NC = bool
		
    Char.Animate.Enabled = not NC
    if NC then
        spawn(function()
            
            NC_Clip = game:GetService("RunService").Stepped:Connect(function()
                
            end)
            
        end)
        

        Heart[#Heart + 1] = game["Run Service"].Heartbeat:Connect(function()
            if Forward then 
                Player.Character:TranslateBy(Camera.CFrame.lookVector*VELOCIDADE) 
            end 
            if Back then 
                Player.Character:TranslateBy(-Camera.CFrame.lookVector*VELOCIDADE) 
            end 
            if Left then 
                Player.Character:TranslateBy(-Camera.CFrame:vectorToWorldSpace(Vector3.new(1,0,0))*VELOCIDADE) 
            end 
            if Right then 
                Player.Character:TranslateBy(Camera.CFrame:vectorToWorldSpace(Vector3.new(1,0,0))*VELOCIDADE) 
            end 
            if Up then Player.Character:TranslateBy(Camera.CFrame:vectorToWorldSpace(Vector3.new(0,1,0))*VELOCIDADE) 
            end 
            if Down then 
                Player.Character:TranslateBy(-Camera.CFrame:vectorToWorldSpace(Vector3.new(0,1,0))*VELOCIDADE) 
            end  
        end)
        spawn(function()
            while NC --[[Dont need "==true"]] do
            Char=game.Players.LocalPlayer.Character
                Char.HumanoidRootPart.Velocity = Vector3.new(0,1.5,0)
                task.wait()
            end
        end)
        
    else
        for i,v in pairs(Heart) do v:Disconnect() end
    end
end)


local VisibleEsp = false
local VisibleLine = false


local esps = {}
function EspPlayer(plr)
    local Esp = Drawing.new("Text")
    
    Esp.Center = true
    Esp.Outline = true
    Esp.OutlineColor = Color3.fromRGB(255,255,255)
    Esp.Font = 2

    local Line = Drawing.new("Line")
    Line.Thickness = 1.5
    Line.Visible = false
    
    coroutine.wrap(function()
        loop = game:GetService("RunService").RenderStepped:Connect(function()
            local sucess = pcall(function()
                local Vector, OnScreen = workspace.CurrentCamera:worldToViewportPoint(plr.Character.Head.Position + Vector3.new(0, 4, 0))
                Line.From = Vector2.new(workspace.CurrentCamera.ViewportSize.X / 2, 0)
                Line.To = Vector2.new(Vector.X, Vector.Y)
                local iswhite = false
                local plrStatus = plr.leaderstats.Status
                if plrStatus.Value == "Criminal" then
                    Line.Color = Color3.fromRGB(130, 16, 16)
                elseif plrStatus.Value == "Lawbreaker" then
                    Line.Color = Color3.fromRGB(252, 107, 3)
                elseif plrStatus.Value == "Guardian" then
                    Line.Color = Color3.fromRGB(0, 144, 176)
                elseif plrStatus.Value == "Protector" then
                    Line.Color = Color3.fromRGB(201, 255, 5)
                elseif plrStatus.Value == "Supervillain" then
                    Line.Color = Color3.fromRGB(255,0,0)
                elseif plrStatus.Value == "Superhero" then
                    Line.Color = Color3.fromRGB(7, 2, 250)
                else
                    iswhite = true
                    Line.Color = Color3.fromRGB(255,255,255)
                end
                if not iswhite then
                    Esp.Color = Line.Color
                else
                    Esp.Color = Color3.fromRGB(0, 0, 0)
                end
                
                local diff  = (plr.Character.Head.Position - speaker.Character.HumanoidRootPart.Position)

                local num = (diff.Magnitude - (diff.Magnitude / 2))
                if num < 0 then
                    num = 15
                end
                local CustomSize = diff.Magnitude / (num)

                if CustomSize < 15 then
                    CustomSize = 15
                end
                if CustomSize > 18 then
                    CustomSize = 18
                end
                Esp.Size = CustomSize

                if OnScreen then
                    if getgenv().espmax == true and (plr.Character.Head.Position - game.Players.LocalPlayer.Character.Head.Position).Magnitude < 150 then

                        Esp.Visible = VisibleEsp
                        Line.Visible = VisibleLine
                    elseif getgenv().espmax == false then
                        Esp.Visible = VisibleEsp
                        Line.Visible = VisibleLine
                    else
                        Esp.Visible = false
                        Line.Visible = false
                    end
                    Esp.Position = Vector2.new(Vector.X, Vector.Y)
                    local TabRanks = {
                        [1] = "Blue I";
                        [2] = "Blue II";
                        [3] = "Blue III";
                        [4] = "Red I";
                        [5] = "Red II";
                        [6] = "Red III";
                        [7] = "Yellow I";
                        [8] = "Yellow II";
                        [9] = "Yellow III";
                        [10] = "Sword";
                        [11] = "Crown"
                    }
                    TabRanks = TabRanks[plr.PrivateStats.Rank.Value]
                    Esp.Text = string.format(
                        "Nome: %s\nReputaÃ§Ã£o: %s\nFirst: %s  Body: %s  Psy: %s\nTokens: %s\nRank %s",
                        plr.Name,
                        plr.leaderstats.Status.Value,
                        Translate(plr.PrivateStats.FistStrength.Value),
                        Translate(plr.PrivateStats.BodyToughness.Value),
                        Translate(plr.PrivateStats.PsychicPower.Value),
                        Translate(plr.PrivateStats.Token.Value),
                        TabRanks
                        
                        
                    )
                else
                    Esp.Visible = false
                    Line.Visible = false
                end
            end)
            
            if sucess then
                
            else
                Esp.Visible = false
                Line.Visible = false
            end
                
        end)
        esps[#esps + 1] = {name = plr.Name; Line = Esp; Loop = loop}
        esps[#esps + 1] = {name = plr.Name; Line = Line; Loop = loop}
        
    end)()
end
function RemoveEsp(plr)
    for i,v in pairs(esps) do
        if v.name == plr.Name then
            v.Loop:Disconnect()
            v.Line:Remove()
        end
    end
end

for i,v in pairs(game.Players:GetChildren()) do
    if v ~= speaker then
        EspPlayer(v)
    end
end

game.Players.PlayerAdded:Connect(function(plr)
    EspPlayer(plr)
end)

game.Players.ChildRemoved:Connect(function(plr)
    RemoveEsp(plr)
end)

