    function getBest(tab)
        local pos, tool, location
        for i,v in pairs(tab) do
            if typeof(v) == "table" and v.stats.Value > v.number then
                pos = v.cframe
                location = i
                tool = tab[1]
            end
        end
        return pos, tool, location
