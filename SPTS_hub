farms:CreateToggle('Farm time not to die', function(bool)
    farm_time = bool
    spawn(function()
        if bool then
            oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            while farm_time do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(math.random(-1000, 1000), math.random(5000, 30000), math.random(-100, 1000))
                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                task.wait()
            end
        else
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldpos
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
        end
    end)
end)

